  width: 24px;
  height: 24px;
  position: absolute;
  left: -999em;
  top: -999em;
  z-index: 2;
  pointer-events: none;
  display: none;
  opacity: 0;
  transform-origin: 0 0; }

.libras-spinner::before {
  position: absolute;
  margin: -50% 0 0 -50%;
  width: 100%;
  height: 100%;
  background: url("../base/images/Loading_Indicator_double_32.svg") no-repeat center;
  display: block;
  content: '';
  animation: libras-spinner--rotate-animation 750ms linear infinite; }

.libras-spinner--start {
