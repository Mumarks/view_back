.libras-view {
  display: flex;
  overflow: hidden;
  margin: 0;
  padding: 0;
  /*
   * CSS2 cursors:
   * auto, inherit, crosshair, default, help, move, pointer , progress, text, wait,
   * e-resize, ne-resize, nw-resize, n-resize, se-resize, sw-resize, s-resize, w-resize
   *
   * CSS3 cursors:
   * vertical-text, no-drop, not-allowed,
   * ew-resize, ns-resize, nesw-resize, nwse-resize, col-resize, row-resize, all-scroll
   *
   * CSS3 with limited support:
   * - cursor: none (not IE, Safari, Opera)
   * - cursor: context-menu (not Firefox, Chrome)
   * - cursor: cell (not Safari)
   * - cursor: alias (not Safari)
   * - cursor: copy (not Safari)
   *
   * Browser specific:
   * - cursor: -webkit-grab; cursor: -moz-grab;
   * - cursor: -webkit-grabbing; cursor: -moz-grabbing;
   * - cursor: -webkit-zoom-in; cursor: -moz-zoom-in;
   * - cursor: -webkit-zoom-out; cursor: -moz-zoom-out;
   *
   * Supported cursors:
   * default, crosshair, help, move, pointer, progress, grab, grabbing
   */ }
  .libras-view .libras-view-root {
    position: relative;
    flex: 1 1 100%;
    border: none;
    overflow: hidden;
    padding: 0;
    margin: 0;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-tap-highlight-color: transparent; }
  .libras-view .libras-view-surface {
    position: absolute;
    border: none;
    padding: 0;
    margin: 0;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    /*
     * TODO
     * Layers should probably manage this on their own
     */
    direction: ltr; }
  .libras-view .libras-view-surface[data-cursor='default'] {
    cursor: default; }
  .libras-view .libras-view-surface[data-cursor='copy'] {
    cursor: copy; }
  .libras-view .libras-view-surface[data-cursor='crosshair'] {
    cursor: crosshair; }
  .libras-view .libras-view-surface[data-cursor='help'] {
    cursor: help; }
  .libras-view .libras-view-surface[data-cursor='move'] {
    cursor: move; }
  .libras-view .libras-view-surface[data-cursor='pointer'] {
    cursor: pointer; }
  .libras-view .libras-view-surface[data-cursor='progress'] {
    cursor: progress; }
  .libras-view .libras-view-surface[data-cursor='grab'] {
    cursor: move;
    cursor: grab;
    cursor: -moz-grab;
    cursor: -webkit-grab; }
  .libras-view .libras-view-surface[data-cursor='grabbing'] {
    cursor: move;
    cursor: grabbing;
    cursor: -moz-grabbing;
    cursor: -webkit-grabbing; }
  .libras-view .libras-view-surface[data-interacting='true'] {
    cursor: move;
    cursor: grabbing;
    cursor: -moz-grabbing;
    cursor: -webkit-grabbing; }
  .libras-view .libras-display-object,
  .libras-view .libras-bitmap {
    position: absolute;
    border: none;
    margin: 0;
    padding: 0;
    -webkit-user-drag: none;
    -webkit-user-select: none; }
  .libras-view .libras-bitmap {
    max-width: none;
    transform-origin: top left; }

.libras-ui {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  box-sizing: border-box;
  pointer-events: none;
  /* attribution's position is a special case */ }
  .libras-ui .libras-component {
    pointer-events: auto; }
  .libras-ui .libras-attribution {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0; }

.libras-ui-corner {
  position: absolute;
  display: flex; }

.libras-ui-inner-container {
  position: absolute;
  left: 15px;
  top: 15px;
  bottom: 15px;
  right: 15px; }

.libras-ui-manual-container > .libras-component {
  position: absolute; }

